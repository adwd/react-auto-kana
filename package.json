{
  "name": "react-auto-kana",
  "version": "0.0.1",
  "description": "Build Japanese Furigana from input values",
  "homepage": "http://suisho.github.io/react-auto-kana/example/www/",
  "main": "dist/index.js",
  "dependencies": {
    "compact-diff": "^0.0.2",
    "diff": "^1.3.0",
    "extend": "^2.0.0",
    "japanese": "^0.2.1",
    "react": "^0.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:suisho/react-auto-kana.git"
  },
  "devDependencies": {
    "babel": "^4.7.3",
    "babelify": "^5.0.3",
    "browserify": "^8.1.3",
    "eslint": "^0.15.1",
    "eslint-plugin-react": "^1.2.2",
    "espower-loader": "^0.10.0",
    "harp": "^0.15.2",
    "intelli-espower-loader": "^0.6.0",
    "jsx": "^0.9.89",
    "minimist": "^1.1.0",
    "power-assert": "^0.10.2",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "scripts": {
    "lint": "eslint js/**/*.jsx",
    "example": "harp server example",
    "page-build": "browserify example/main.jsx > pages/dist/build.js",
    "page-compile": "harp compile example pages",
    "watch": "watchify -v -d example/main.jsx -o example/dist/build.js",
    "page": " npm run page-compile; npm run page-build",
    "test": "mocha --compilers js:babel/register --require intelli-espower-loader",
    "dist": "mkdir -p dist ; rm dist/*.js; babel src/AutoKana.jsx > dist/index.js; cp src/*.js dist/"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "japanese"
  ],
  "author": "suisho",
  "license": "ISC",
  "browserify": {
    "transform": [
      "babelify"
    ],
    "fullPaths": false
  }
}
